InfoTab:CreateButton({
    Name = "ğŸ” Cek Rod yang Dimiliki",
    Callback = function()
        updateStatus("ğŸ” Memeriksa rod di backpack...")
        
        -- Coba cek di berbagai tempat
        local backpack = localPlayer:FindFirstChild("Backpack")
        local playerGui = localPlayer:FindFirstChild("PlayerGui")
        
        local foundRods = {}
        
        if backpack then
            for _, item in pairs(backpack:GetChildren()) do
                if string.find(string.lower(item.Name), "rod") then
                    table.insert(foundRods, item.Name)
                end
            end
        end
        
        if #foundRods > 0 then
            updateStatus("ğŸ£ ROD DI BACKPACK:")
            for i, rod in ipairs(foundRods) do
                updateStatus(i .. ". " .. rod)
            end
        else
            updateStatus("âŒ Tidak ada rod di backpack")
        end
        
        -- Tampilkan rod yang berhasil dibeli
        if #RodBuyer.PurchasedRods > 0 then
            updateStatus("\nğŸ›ï¸ ROD BERHASIL DIBELI:")
            for i, rod in ipairs(RodBuyer.PurchasedRods) do
                updateStatus(i .. ". " .. rod)
            end
        end
    end
})

InfoTab:CreateButton({

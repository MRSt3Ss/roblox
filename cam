            local function printStructure(folder, depth)
                local prefix = string.rep("  ", depth)
                for _, item in pairs(folder:GetChildren()) do
                    updateStatus(prefix .. "ğŸ“„ " .. item.Name .. " (" .. item.ClassName .. ")")

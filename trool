    Name = "Initialize Fish It V2",
    Parent = FishSection,
    Callback = function()
        if FishItV2:Initialize() then
            updateStatus("Fish It V2 berhasil diinisialisasi!")
        else
            updateStatus("Gagal menginisialisasi Fish It V2!")
        end
